{
  "name": "mcp-server-manager",
  "version": "0.1.0",
  "description": "MCP Server Process Manager GUI",
  "main": "dist/main/index.js",
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=4.0.0",
    "npm": "please-use-yarn",
    "pnpm": "please-use-yarn"
  },
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "concurrently -k -r \"yarn run dev:main\" \"yarn run dev:renderer\" \"yarn run dev:electron\"",
    "dev:main": "tsc -w -p tsconfig.main.json",
    "dev:renderer": "vite",
    "dev:electron": "wait-on dist/main/index.js http://localhost:3001 && electron . --dev",
    "build": "tsc -p tsconfig.main.json && vite build",
    "start": "electron .",
    "dist": "yarn build && electron-builder",
    "dist:win": "yarn build && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false ELECTRON_BUILDER_DISABLE_CODE_SIGNING=true electron-builder --win",
    "dist:mac": "yarn build && electron-builder --mac",
    "dist:linux": "yarn build && electron-builder --linux"
  },
  "keywords": [
    "mcp",
    "server",
    "process",
    "manager"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.4",
    "@mui/material": "^5.15.4",
    "electron-store": "^8.1.0",
    "i18next": "^23.7.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.21.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zustand": "^4.4.7"
  }
}
